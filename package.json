{
    "name": "@team-abyss-p2/finnby",
    "version": "0.1.1",
    "description": "A React framework for Source Panorama",
    "main": "dist/runtime.js",
    "types": "dist/index.d.ts",
    "bin": {
        "finnby": "dist/cli.js"
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "stylecheck": "prettier --check {cli,runtime}/**/*.{ts,tsx,js}",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build": "rollup -c",
        "test": "yarn build && yarn typecheck && yarn lint && yarn stylecheck"
    },
    "repository": "https://github.com/team-abyss-p2/finnby/",
    "author": "l3ops <github@leops.me>",
    "license": "MIT",
    "files": [
        "dist",
        "runtime/panorama.d.ts"
    ],
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "all",
        "proseWrap": "always"
    },
    "devDependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.3",
        "@types/react-reconciler": "^0.26.1",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "eslint": "^7.28.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.2.1",
        "react-reconciler": "^0.26.2",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "cosmiconfig": "^7.0.0",
        "postcss": "^8.2.14",
        "postcss-advanced-variables": "^3.0.1",
        "postcss-atroot": "^0.1.3",
        "postcss-extend-rule": "^3.0.0",
        "postcss-modules": "^4.0.0",
        "postcss-nested": "^5.0.5",
        "postcss-property-lookup": "^2.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^2.47.0"
    }
}
